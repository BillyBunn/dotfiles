{
    "extends": [
        "wesbos"
    ],
    "rules": {
        "camelcase": [
            "warn",
            {
                "properties": "never",
                "ignoreDestructuring": true
            }
        ],
        "no-plusplus": [
            "error",
            {
                "allowForLoopAfterthoughts": true
            }
        ]
    }
}